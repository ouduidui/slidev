{
  "rewrites": [
    {
      "source": "/demo/(.*)",
      "destination": "/demo/index.html"
    },
    {
      "source": "/note-app-simple-share/(.*)",
      "destination": "/note-app-simple-share/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}